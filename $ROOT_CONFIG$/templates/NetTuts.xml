<templateSet group="NetTuts">
  <template name="c" value="[$CODE$]$END$[$CODE$]" description="Code Block in Nettuts Tutorials" toReformat="false" toShortenFQNames="true">
    <variable name="CODE" expression="" defaultValue="&quot;PHP&quot;" alwaysStopAt="true" />
    <context>
      <option name="HTML_TEXT" value="true" />
      <option name="HTML" value="true" />
      <option name="XSL_TEXT" value="false" />
      <option name="XML" value="false" />
      <option name="CSS_PROPERTY_VALUE" value="false" />
      <option name="CSS_DECLARATION_BLOCK" value="false" />
      <option name="CSS_RULESET_LIST" value="false" />
      <option name="CSS" value="false" />
      <option name="JSP" value="false" />
      <option name="SQL" value="false" />
      <option name="JAVA_SCRIPT" value="false" />
      <option name="TypeScript" value="false" />
      <option name="CoffeeScript" value="false" />
      <option name="PHP" value="false" />
      <option name="HAML" value="false" />
      <option name="OTHER" value="false" />
    </context>
  </template>
  <template name="img" value="&lt;img src=&quot;Images/$IMAGE_FILE$&quot; alt=&quot;$IMAGE_FILE$&quot; width=&quot;600&quot;/&gt;&#10;$END$" description="Add img tag in Nettuts Tutorials" toReformat="false" toShortenFQNames="true">
    <variable name="IMAGE_FILE" expression="complete()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML_TEXT" value="true" />
      <option name="HTML" value="true" />
      <option name="XSL_TEXT" value="false" />
      <option name="XML" value="false" />
      <option name="CSS_PROPERTY_VALUE" value="false" />
      <option name="CSS_DECLARATION_BLOCK" value="false" />
      <option name="CSS_RULESET_LIST" value="false" />
      <option name="CSS" value="false" />
      <option name="JSP" value="false" />
      <option name="SQL" value="false" />
      <option name="JAVA_SCRIPT" value="false" />
      <option name="TypeScript" value="false" />
      <option name="CoffeeScript" value="false" />
      <option name="PHP" value="false" />
      <option name="HAML" value="false" />
      <option name="OTHER" value="false" />
    </context>
  </template>
  <template name="st" value="&lt;hr/&gt;&#10;&lt;h2&gt;$TITLE$&lt;/h2&gt;&#10;&#10;&lt;p&gt;$END$&lt;/p&gt;&#10;" description="New Step in Nettuts Tutorials" toReformat="true" toShortenFQNames="true">
    <variable name="TITLE" expression="" defaultValue="Title" alwaysStopAt="true" />
    <context>
      <option name="HTML_TEXT" value="true" />
      <option name="HTML" value="true" />
      <option name="XSL_TEXT" value="false" />
      <option name="XML" value="false" />
      <option name="CSS_PROPERTY_VALUE" value="false" />
      <option name="CSS_DECLARATION_BLOCK" value="false" />
      <option name="CSS_RULESET_LIST" value="false" />
      <option name="CSS" value="false" />
      <option name="JSP" value="false" />
      <option name="SQL" value="false" />
      <option name="JAVA_SCRIPT" value="false" />
      <option name="TypeScript" value="false" />
      <option name="CoffeeScript" value="false" />
      <option name="PHP" value="false" />
      <option name="HAML" value="false" />
      <option name="OTHER" value="false" />
    </context>
  </template>
</templateSet>