<application>
  <component name="FileTypeManager" version="11">
    <ignoreFiles list="CVS;SCCS;RCS;rcs;.DS_Store;.svn;.pyc;.pyo;*.pyc;*.pyo;.git;*.hprof;_svn;.hg;*.lib;*~;__pycache__;.bundle;*.rbc;*.orig;*.o;*.obj;" />
    <extensionMap>
      <mapping ext="ctp" type="PHP" />
      <mapping pattern="Makefile" type="PLAIN_TEXT" />
    </extensionMap>
    <setting name="fileTypeChangedCounter" value="1" />
  </component>
</application>